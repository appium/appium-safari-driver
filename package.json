{
  "name": "appium-safari-driver",
  "description": "Appium driver for safari browsers",
  "keywords": [
    "appium",
    "safari"
  ],
  "version": "2.0.0",
  "author": "appium",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/appium-safari-driver.git"
  },
  "bugs": {
    "url": "https://github.com/appium/appium-safari-driver/issues"
  },
  "engines": [
    "node"
  ],
  "appium": {
    "driverName": "safari",
    "automationName": "Safari",
    "platformNames": [
      "Mac",
      "Windows",
      "iOS"
    ],
    "mainClass": "SafariDriver"
  },
  "main": "./build/index.js",
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "appium-base-driver": "^5.8.0",
    "appium-support": "*",
    "asyncbox": "*",
    "babel-runtime": "^6.26.0",
    "bluebird": "*",
    "lodash": "*",
    "source-map-support": "^0.5.18",
    "teen_process": "*",
    "yargs": "^15.3.1"
  },
  "scripts": {
    "build": "gulp transpile",
    "prepare": "gulp transpile",
    "mocha": "mocha",
    "prepublish": "gulp prepublish",
    "test": "gulp once",
    "e2e-test": "gulp e2e-test",
    "watch": "gulp watch",
    "coverage": "gulp coveralls",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit-test": "REPORTER=dot gulp once",
    "lint": "gulp eslint",
    "lint:fix": "gulp eslint --fix"
  },
  "pre-commit": [
    "precommit-msg",
    "precommit-test"
  ],
  "devDependencies": {
    "appium-gulp-plugins": "^5.2.0",
