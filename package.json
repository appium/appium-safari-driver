    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-appium": "^4.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "*",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "*",
    "gulp": "^4.0.2",
    "mocha": "^7.1.1",
    "pre-commit": "*",
    "sinon": "^9.0.2",
    "wd": "*"
  }
